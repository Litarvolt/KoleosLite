@page "/"
@namespace KoleosDemo.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!doctype html>
<html lang="en">
<head>
 <meta charset="utf-8" />
 <meta name="viewport" content="width=device-width, initial-scale=1.0" />
 <title>KoleosDemo Dashboard</title>
 <base href="/" />
 <!-- Bootstrap5 -->
 <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="" crossorigin="anonymous" />
 <!-- Font Awesome -->
 <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet" />
 <link href="css/bootstrap.min.css" rel="stylesheet" />
 <link href="css/site.css" rel="stylesheet" />
</head>
<body>
 <app>
 <component type="typeof(App)" render-mode="ServerPrerendered" />
 </app>
 <script>
 // Normalize duplicate slashes in path before Blazor loads
 (function(){
 var p = location.pathname || '/';
 if (p.indexOf('//') !== -1) {
 var normalized = p.replace(/\/\/{2,}/g, '/');
 var newUrl = location.protocol + '//' + location.host + normalized + location.search + location.hash;
 console.debug('Normalizing URL from', location.href, 'to', newUrl);
 location.replace(newUrl);
 }
 })();
 </script>
 <script src="/js/theme.js"></script>
 <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="" crossorigin="anonymous"></script>
 <script src="_framework/blazor.server.js"></script>
</body>
</html>